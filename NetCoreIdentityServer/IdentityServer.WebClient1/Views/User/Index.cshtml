@model UserConfigureViewModel 
@{
    ViewData["Title"] = "Index";
}

<h3>Bu sayfayı üye olan kullanıcılar görebilir.</h3>
<hr />

<h5>Claim Özellikleri</h5>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

<hr />
<h4 class="mx-3">Authentication Verileri</h4>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Key</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.AuthenticationItems)
        {
            <tr>
                <td>@claim.Key</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>